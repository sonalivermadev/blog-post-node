<h1>All posts</h1>

<table>
  <thead>
    <tr>
      <th>title</th>
      <th>body</th>
      <th>photo</th>
      <th>postedBy</th>
      <th>date</th>
    </tr>
  </thead>

  <tr>
    <% for (var i = 0; i < posts.length; i++) { %>
    <td><%=posts[i].title%></td>
    <td><%=posts[i].body%></td>
    <td><%=posts[i].photo%></td>
    <td><%=posts[i].postedBy%></td>
    <td><%=posts[i].date%></td>
  </tr>
  <% } %>
</table>
